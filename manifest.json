{
  "name": "jav_bot",
  "title": "精品学习资料",
  "author": "oreki",
  "description": "",
  "version": "1.2",
  "requirements": [],
  "configField": [
    {
      "fieldName": "proxy",
      "fieldType": "String",
      "label": "代理",
      "defaultValue": ""
    },
    {
      "fieldName": "qb_name",
      "fieldType": "String",
      "label": "请填写下载工具中,QB的名称，目前只支持QB下载",
      "defaultValue": ""
    },
    {
      "fieldName": "path",
      "fieldType": "String",
      "label": "qb手动模式下,请填入保存路径",
      "defaultValue": ""
    },
    {
      "fieldName": "category",
      "fieldType": "String",
      "label": "qb自动模式下,请填入分类名称,请提前在qb中建好分类,否贼将保存在默认目录下",
      "defaultValue": ""
    },
    {
      "fieldName": "need_hard_link",
      "fieldType": "Bool",
      "label": "硬链",
      "helperText": "是否开启硬链",
      "defaultValue": false
    },
    {
      "fieldName": "hard_link_dir",
      "fieldType": "String",
      "label": "设置硬链的目录,请提前创建硬链的目录，否则硬链会失败",
      "defaultValue": ""
    },
    {
      "fieldName": "need_mdc",
      "fieldType": "Bool",
      "label": "MDC",
      "helperText": "是否开启MDC整理学习资料,整理的目录取决于是否开硬链",
      "defaultValue": false
    },
    {
      "fieldName": "mdc_exclude_dir",
      "fieldType": "String",
      "label": "排除需要整理的目录",
      "defaultValue": ""
    },
    {
      "fieldName": "jav_cookie",
      "fieldType": "String",
      "label": "javlibrary的cookie,倘如遇到cf盾，请设置cookie",
      "defaultValue": ""
    },
    {
      "fieldName": "ua",
      "fieldType": "String",
      "label": "倘如遇到cf盾，请设置ua",
      "defaultValue": ""
    },
    {
      "fieldName": "uid",
      "fieldType": "Enum",
      "label": "推送用户",
      "helperText": "选择接收推送的用户,不选则不推送",
      "enumValuesRef": "UserList",
      "multiValue": true
    }
  ],
  "logoUrl": "",
  "githubUrl": null,
  "helpDocUrl": "",
  "dependencies": null
}